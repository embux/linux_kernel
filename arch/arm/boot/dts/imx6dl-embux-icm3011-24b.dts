/dts-v1/;

#include "imx6dl-embux.dtsi"
#include "imx6dl-embux-base-ldb.dtsi"
#include "imx6dl-embux-base-ldb24.dtsi"
#include "imx6dl-embux-icm3011.dtsi"

/ {
	model = "Freescale i.MX6 DualLite embux wcp-icm3011 board";
	compatible = "fsl,imx6dl-sabresd", "fsl,imx6dl";
};

&i2c2 {
	pmic: pfuze100@08 {
		compatible = "fsl,pfuze100";
		reg = <0x08>;

		regulators {
			sw1a_reg: sw1ab {
				regulator-min-microvolt = <1375000>;
				regulator-max-microvolt = <1375000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw1c_reg: sw1c {
				regulator-min-microvolt = <1375000>;
				regulator-max-microvolt = <1375000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw2_reg: sw2 {
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw3a_reg: sw3a {
				regulator-min-microvolt = <1500000>;
				regulator-max-microvolt = <1500000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw3b_reg: sw3b {
				regulator-min-microvolt = <1975000>;
				regulator-max-microvolt = <1975000>;
				regulator-boot-on;
				regulator-always-on;
				regulator-ramp-delay = <6250>;
			};

			sw4_reg: sw4 {
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-ramp-delay = <6250>;
			};

			swbst_reg: swbst {
				regulator-min-microvolt = <5150000>;
				regulator-max-microvolt = <5150000>;
			};

			snvs_reg: vsnvs {
				regulator-min-microvolt = <3000000>;
				regulator-max-microvolt = <3000000>;
				regulator-boot-on;
				regulator-always-on;
			};

			vref_reg: vrefddr {
				regulator-boot-on;
				regulator-always-on;
			};

			vgen1_reg: vgen1 {
				regulator-min-microvolt = <1550000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen2_reg: vgen2 {
				regulator-min-microvolt = <1550000>;
				regulator-max-microvolt = <1550000>;
			};

			vgen3_reg: vgen3 {
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
			};

			vgen4_reg: vgen4 {
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
			};

			vgen5_reg: vgen5 {
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};

			vgen6_reg: vgen6 {
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
			};
		};
	};
};

&i2c3 {
	rtc: rx8010@32 {
		compatible = "epson,rx8010";
		reg = <0x32>;
		status = "okay";
		//interrupt-parent = <&gpio2>, <&gpio4>;
		//interrupts = <17>, <19>;
		//rx8010-irq_1 = <&gpio2 17 0>;	
		//rx8010-irq_2 = <&gpio4 19 0>;		
	};
};

&battery {
	offset-charger = <1485>;
	offset-discharger = <1464>;
	offset-usb-charger = <1285>;
};



&epdc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_epdc_ebx_ea2>, <&pinctrl_gpio1_ebx_icm3011>, <&pinctrl_gpio2_ebx_icm2011>, <&pinctrl_gpio3_ebx_icm3011>,
				<&pinctrl_gpio4_ebx_wcp2410>, <&pinctrl_gpio5_ebx_wcp2410>, <&pinctrl_gpio6_ebx_icm3011>, <&pinctrl_gpio7_ebx_wcp2410>;
	V3P3-supply = <&vgen6_reg>; 
	//VCOM-supply = <&VCOM_reg>;
	//DISPLAY-supply = <&DISPLAY_reg>;
	status = "okay";
};


&ldb {
	status = "okay";
	lvds-channel@0 {
		crtc = "ipu1-di1";
	};

	lvds-channel@1 {
		crtc = "ipu1-di0";
	};
};


&mxcfb1 {
	status = "okay";
};

&mxcfb2 {
	status = "okay";
};

&mxcfb3 {
	status = "okay";
};

&pxp {
	status = "okay";
};
