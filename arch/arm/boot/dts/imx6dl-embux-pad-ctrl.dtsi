#ifndef __DTSI_IMX6DL_PAD_CTRL_VALUE_H__
#define __DTSI_IMX6DL_PAD_CTRL_VALUE_H__

// common flag
#define EBX_PAD_CTL_HYS				(1 << 16)

#define EBX_PAD_CTL_PKE				(1 << 12)
#define EBX_PAD_CTL_PUE				(1 << 13)
#define EBX_PAD_CTL_ODE				(1 << 11)

#define EBX_PAD_CTL_PUS_100K_DOWN	(0 << 14)
#define EBX_PAD_CTL_PUS_47K_UP		(1 << 14)
#define EBX_PAD_CTL_PUS_100K_UP		(2 << 14)
#define EBX_PAD_CTL_PUS_22K_UP		(3 << 14)

#define EBX_PAD_CTL_SPEED_LOW		(1 << 6)
#define EBX_PAD_CTL_SPEED_MED		(2 << 6)
#define EBX_PAD_CTL_SPEED_HIGH		(3 << 6)

#define EBX_PAD_CTL_DSE_DISABLE		(0 << 3)
#define EBX_PAD_CTL_DSE_240ohm		(1 << 3)
#define EBX_PAD_CTL_DSE_120ohm		(2 << 3)
#define EBX_PAD_CTL_DSE_80ohm		(3 << 3)
#define EBX_PAD_CTL_DSE_60ohm		(4 << 3)
#define EBX_PAD_CTL_DSE_48ohm		(5 << 3)
#define EBX_PAD_CTL_DSE_40ohm		(6 << 3)
#define EBX_PAD_CTL_DSE_34ohm		(7 << 3)

#define EBX_PAD_CTL_SRE_FAST		(1 << 0)
#define EBX_PAD_CTL_SRE_SLOW		(0 << 0)



#define EBX_NO_PAD_CTRL				0x80000000 //(1 << 17)
// 1b0b0
#define EBX_ENET_PAD_CTRL 			(EBX_PAD_CTL_PKE | EBX_PAD_CTL_PUE | EBX_PAD_CTL_PUS_100K_UP | \
									EBX_PAD_CTL_SPEED_MED | EBX_PAD_CTL_DSE_40ohm | EBX_PAD_CTL_HYS)
// 0a0b1
#define EBX_ENET_REF_CLK_PAD_CTRL	(EBX_PAD_CTL_PUE | EBX_PAD_CTL_PUS_100K_UP | EBX_PAD_CTL_SPEED_MED | \
									EBX_PAD_CTL_DSE_40ohm | EBX_PAD_CTL_SRE_FAST)
// 1b8b1
#define EBX_I2C_PAD_CTRL			(0x40000000 | EBX_PAD_CTL_PKE | EBX_PAD_CTL_PUE | EBX_PAD_CTL_PUS_100K_UP | \
									EBX_PAD_CTL_SPEED_MED | EBX_PAD_CTL_DSE_40ohm | EBX_PAD_CTL_SRE_FAST | \
									EBX_PAD_CTL_HYS | EBX_PAD_CTL_ODE)
									
#define EBX_I2C_PAD_CTRL_2410		(0x40000000 | EBX_PAD_CTL_PKE | EBX_PAD_CTL_PUE | EBX_PAD_CTL_PUS_100K_UP | \
									EBX_PAD_CTL_SPEED_MED | EBX_PAD_CTL_DSE_34ohm | EBX_PAD_CTL_SRE_FAST | \
									EBX_PAD_CTL_HYS | EBX_PAD_CTL_ODE)									
// 1b0b1									
#define EBX_UART_PAD_CTRL			(EBX_PAD_CTL_PKE | EBX_PAD_CTL_PUE | EBX_PAD_CTL_PUS_100K_UP | \
									EBX_PAD_CTL_SPEED_MED | EBX_PAD_CTL_DSE_40ohm | EBX_PAD_CTL_HYS | \
									EBX_PAD_CTL_SRE_FAST)
// 13001									
#define EBX_USDHC_PAD_CTRL_COMMON	(EBX_PAD_CTL_PKE | EBX_PAD_CTL_PUE | EBX_PAD_CTL_HYS | \
									EBX_PAD_CTL_SRE_FAST | EBX_PAD_CTL_HYS)	
// 13001 | 0c070 =	1f071
#define EBX_USDHC_PAD_CTRL			(EBX_USDHC_PAD_CTRL_COMMON | EBX_PAD_CTL_PUS_22K_UP | EBX_PAD_CTL_SPEED_LOW | \
									EBX_PAD_CTL_DSE_40ohm)
// 13001 | 04070 =	17071
#define EBX_USDHC_PAD_CTRL_50MHZ_40OHM	(EBX_USDHC_PAD_CTRL_COMMON | EBX_PAD_CTL_PUS_47K_UP | EBX_PAD_CTL_SPEED_LOW | \
									EBX_PAD_CTL_DSE_40ohm)
// 170b1
#define EBX_USDHC_PAD_CTRL_100MHZ	(EBX_USDHC_PAD_CTRL_COMMON | EBX_PAD_CTL_PUS_47K_UP | EBX_PAD_CTL_SPEED_MED | \
									EBX_PAD_CTL_DSE_40ohm)									
// 170f9
#define EBX_USDHC_PAD_CTRL_200MHZ	(EBX_USDHC_PAD_CTRL_COMMON | EBX_PAD_CTL_PUS_47K_UP | EBX_PAD_CTL_SPEED_HIGH | \
									EBX_PAD_CTL_DSE_34ohm)				

#define EBX_AUD_PAD_CTRL			(EBX_PAD_CTL_PKE | EBX_PAD_CTL_PUE | EBX_PAD_CTL_DSE_40ohm | EBX_PAD_CTL_PUS_100K_DOWN | \
									EBX_PAD_CTL_HYS | EBX_PAD_CTL_SPEED_MED)
							
#endif
